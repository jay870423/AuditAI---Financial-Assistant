{
  "rewrites": [
    {
      "source": "/gemini-api/:match*",
      "destination": "https://generativelanguage.googleapis.com/:match*"
    },
    {
      "source": "/openai-api/:match*",
      "destination": "https://api.openai.com/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}